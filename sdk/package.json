{
  "name": "@gmx-io/sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "build/src/index.js",
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "require": "./build/src/index.js"
    },
    "./abis/*": "./build/abis/*",
    "./prebuilt/*": "./build/prebuilt/*",
    "./utils/*": "./build/src/utils/*",
    "./types/*": "./build/src/types/*",
    "./configs/*": "./build/src/configs/*"
  },
  "scripts": {
    "prebuild": "yarn tsx scripts/prebuild",
    "prepare": "ts-patch install -s",
    "copy:jsons": "cp-cli src/abis build/src && cp-cli src/prebuilt build/src",
    "test": "jest",
    "tscheck": "tsc -p tsconfig.json --noEmit",
    "build": "rimraf build && tsc -p tsconfig.json && yarn copy:jsons"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "testTimeout": 15000,
    "testPathIgnorePatterns": [
      "/build/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^configs/(.*)$": "<rootDir>/src/configs/$1",
      "^modules/(.*)$": "<rootDir>/src/modules/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^types/(.*)$": "<rootDir>/src/types/$1",
      "prebuilt": "<rootDir>/src/prebuilt/index.ts",
      "^abis/(.*)$": "<rootDir>/src/abis/$1"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cross-fetch": "4.0.0",
    "graphql": "15.8.0",
    "isomorphic-performance": "5.1.1",
    "lodash": "4.17.21",
    "query-string": "7.1.1",
    "typescript": "5.4.2",
    "universal-perf-hooks": "1.0.1",
    "viem": "2.7.20"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/jest": "29.5.13",
    "@types/lodash": "4.14.198",
    "@types/node": "18.7.13",
    "cp-cli": "2.0.0",
    "jest": "29.7.0",
    "lint-staged": "12.3.4",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "ts-patch": "3.2.1",
    "tsx": "4.19.0",
    "typescript-transform-paths": "3.5.1"
  },
  "files": [
    "build"
  ],
  "packageManager": "yarn@3.1.0"
}
